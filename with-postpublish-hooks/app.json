{
  "expo": {
    "name": "with-postpublish-hooks",
    "slug": "with-postpublish-hooks",
    "version": "1.0.0",
    "hooks": {
      "postPublish": [
        {
          "file": "./hooks/echo",
          "config": {
            "message": "hello brent"
          }
        },
        {
          "file": "expo-postpublish-slack-notify",
          "config": {
            "webhookUrl": "https://hooks.slack.com/your-webhook-info-here"
          }
        }
      ]
    }
  }
}
